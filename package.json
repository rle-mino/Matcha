{
  "name": "matcha",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "babel-node": "babel-node --presets=es2015",
    "start": "nodemon --exec npm run babel-node -- server.js"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "fp"
    ],
    "rules": {
      "indent": 0,
      "newline-per-chained-call": 0,
      "eol-last": 0,
      "fp/no-arguments": "error",
      "fp/no-class": "error",
      "fp/no-delete": "error",
      "fp/no-events": "error",
      "fp/no-get-set": "error",
      "fp/no-let": "error",
      "fp/no-loops": "error",
      "fp/no-mutating-assign": "error",
      "fp/no-mutating-methods": "error",
      "fp/no-mutation": "error",
      "fp/no-proxy": "error",
      "fp/no-rest-parameters": "error",
      "fp/no-this": "error",
      "fp/no-throw": "error",
      "no-var": "error"
    }
  },
  "author": "rle-mino",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "body-parser": "^1.15.2",
    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "express": "^4.14.0",
    "joi": "^9.0.4",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "mongodb": "^2.2.6",
    "nodemailer": "^2.5.0",
    "nodemon": "^1.10.0",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-plugin-fp": "^2.1.0"
  }
}
